{
  "functions": {
    "source": "functions",
    "codebase": "default",
    "timeoutSeconds": 120,
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "*.local"
    ]
  },
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-east1"
    },
    "rewrites": [
      {
        "source": "/reservation",
        "function": "reservation"
      }
    ]
  }
}
